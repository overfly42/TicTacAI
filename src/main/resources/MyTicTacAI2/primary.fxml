<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="406.0" prefWidth="534.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MyTicTacAI2.PrimaryController">
   <children>
      <BorderPane prefHeight="256.0" prefWidth="311.0" VBox.vgrow="ALWAYS">
         <top>
            <Label text="Welcome to Tic Tac Toe" BorderPane.alignment="CENTER" />
         </top>
         <bottom>
            <Label fx:id="stateDisplay" text="Status" BorderPane.alignment="CENTER" />
         </bottom>
         <center>
            <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <ComboBox fx:id="PlayerBSelector" layoutX="333.0" layoutY="38.0" prefWidth="150.0" />
                  <ComboBox fx:id="PlayerASelector" layoutX="81.0" layoutY="38.0" prefWidth="150.0" />
                  <Label layoutX="14.0" layoutY="43.0" text="Spieler A" />
                  <Label layoutX="258.0" layoutY="43.0" text="Spieler B" />
                  <Button fx:id="startStop" layoutX="73.0" layoutY="219.0" onAction="#toggleRunningState" text="Start" />
                  <Spinner fx:id="NumberOfGames" editable="true" layoutX="29.0" layoutY="119.0" />
                  <Label layoutX="26.0" layoutY="84.0" text="Number Of Games:" />
                  <ListView fx:id="ObserverList" layoutX="258.0" layoutY="119.0" prefHeight="200.0" prefWidth="200.0" />
                  <Label layoutX="287.0" layoutY="84.0" text="Observer" />
                  <ComboBox fx:id="startPlayer" layoutX="29.0" layoutY="167.0" prefHeight="26.0" prefWidth="171.0" />
               </children>
            </AnchorPane>
         </center>
      </BorderPane>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
